        {
          "dataset": "movie_reco",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "dfdjk9i3en4e8d"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  i.run_id               AS ingestion_run_id,\r\n  i.started_at           AS ingestion_started_at,\r\n  i.status               AS ingestion_status,\r\n  i.duration_sec         AS ingestion_duration,\r\n  i.rows_movies_injected,\r\n  i.rows_ratings_injected,\r\n  i.rows_movies_total,\r\n  i.rows_ratings_total,\r\n\r\n  t.training_run_id,\r\n  t.training_started_at,\r\n  t.training_status,\r\n  t.training_duration,\r\n  t.training_error\r\n\r\nFROM raw.ingestion_run_log i\r\n\r\nLEFT JOIN LATERAL (\r\n    SELECT\r\n        run_id AS training_run_id,\r\n        started_at AS training_started_at,\r\n        status AS training_status,\r\n        duration_sec AS training_duration,\r\n        error_message AS training_error\r\n    FROM raw.training_run_log\r\n    WHERE started_at >= i.started_at\r\n      AND started_at < i.started_at + INTERVAL '2 hours'\r\n    ORDER BY started_at\r\n    LIMIT 1\r\n) t ON TRUE\r\n\r\nWHERE i.started_at >= '${run_date}'::date\r\n  AND i.started_at < ('${run_date}'::date + INTERVAL '1 day')\r\n\r\nORDER BY i.started_at DESC;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "table": "raw.ingestion_run_log"
        }
      ],
      "title": "Full pipeline View",
      "type": "table"
